[ignore]
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/node_modules/resolve/test/resolver/malformed_package_json/package.json
<PROJECT_ROOT>/node_modules/eslint-plugin-react/node_modules/resolve/test/resolver/malformed_package_json/package.json

[include]

[libs]

[lints]
deprecated-type=error
deprecated-utility=error
implicit-inexact-object=error
nonstrict-import=error
sketchy-null=error
sketchy-number=error
unnecessary-invariant=error
unnecessary-optional-chain=error
unsafe-getters-setters=error
untyped-type-import=error

[options]
emoji=true
exact_by_default=true
experimental.enforce_local_inference_annotations=true
experimental.enforce_this_annotations=true
experimental.enforce_class_annotations=true
inference_mode=constrain_writes
module.name_mapper.extension='mp3' -> '<PROJECT_ROOT>/src/util/mp3-stub.js'
react.runtime=automatic

[strict]
unclear-type
untyped-import
